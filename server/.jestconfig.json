{
  "verbose": false,
  "rootDir": "..",
  "roots": [
    "<rootDir>/server",
    "<rootDir>/shared"
  ],
  "setupFiles": [
    "<rootDir>/__mocks__/console.js",
    "./server/test/helper.js"
  ],
  "testEnvironment": "node",
  "testMatch": [
    "**/__tests__/**/*.[jt]s?(x)",
    "**/?(*.)+(spec|test).[jt]s?(x)",
    "!**/server/env.test.js"
  ]
}